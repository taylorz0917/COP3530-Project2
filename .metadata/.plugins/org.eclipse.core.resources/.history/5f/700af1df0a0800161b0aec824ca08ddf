#include <iostream>
#include <list>
#include <vector>
#include <string>
#include <cstdlib>

using namespace std;

class realm
{
public:
	realm(string charm, vector <int> magi);

private:
	string charm;
	vector <int> magi;
	int gems;
};

realm::realm(string charm, vector <int> magi):charm(charm),magi(magi){
	gems=0;
}



class graph
{
public:

private:
	int vertices;
	list<int> *adj;

};

int main()
{
	int numrealms, nummagi, power;
	string charm, currcharm, destcharm;
	vector <int> vec;

	cin >> numrealms;

	for(int i = 0; i < numrealms; i++)
	{
		realm r;
		cin >> charm;
		cin >> nummagi;

		for(int j = 0; j < nummagi; i++)
		{
			cin >> power;
			vec.push_back(power);
		}
		r.realm(charm,vec);
	}

	cin >> currcharm;
	cin >> destcharm;
}
